# DigiCraft Environment Configuration
# Copy this file to client/.env.local and fill in your actual values

# Google Analytics 4
REACT_APP_GA_MEASUREMENT_ID=G-XXXXXXXXXX

# EmailJS Configuration
REACT_APP_EMAILJS_SERVICE_ID=your_service_id
REACT_APP_EMAILJS_TEMPLATE_ID=your_template_id
REACT_APP_EMAILJS_PUBLIC_KEY=your_public_key

# Calendly Configuration
REACT_APP_CALENDLY_USERNAME=digicraft
REACT_APP_CALENDLY_EVENT_TYPE=free-consultation

# App Configuration
REACT_APP_URL=https://digicraft.space

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_EMAILJS=true
REACT_APP_ENABLE_CALENDLY=true
REACT_APP_ENABLE_I18N=true
